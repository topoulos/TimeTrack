<Window x:Class="PersonalTimeTracker.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525" Background="#FF343D51">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="ContentRegion" />
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="27.146" Margin="10,10,0,0" VerticalAlignment="Top" Width="81.926" Background="#FF3B74B8"/>
    </Grid>
</Window>
